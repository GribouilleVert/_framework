{
    "name": "underscore-blank/underscore-framework",
    "description": "Framework php",
    "type": "framework",
    "license": "Private",
    "authors": [
        {
            "name": "Vasco Compain",
            "email": "vasco.gribouille@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "App\\": "App/",
            "Framework\\": "Framework/"
        },
        "files": [
            "config/constants.php",
            "Framework/Errors/sentry_global.php",
            "Framework/functions.php",
            "App/functions.php"
        ]
    },
    "require": {
        "php": ">=7.4",
        "ext-pdo": "*",
        "league/plates": "^3",
        "php-di/php-di": "^6.3",
        "laminas/laminas-diactoros": "^2",
        "laminas/laminas-httphandlerrunner": "^1",
        "robmorgan/phinx": "^0.12.4",
        "pagerfanta/pagerfanta": "^2.7",
        "altorouter/altorouter": "^2.0",
        "sentry/sdk": "^3.1"
    },
    "require-dev": {
        "middlewares/whoops": "^2",
        "symfony/var-dumper": "^5"
    }
}
